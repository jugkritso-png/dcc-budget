{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "node",
        "target": "ES2019",
        "noEmit": false,
        "isolatedModules": false,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "outDir": ".vercel/output",
        "rootDir": "..",
        "skipLibCheck": true
    },
    "include": [
        "**/*",
        "../server/**/*"
    ]
}